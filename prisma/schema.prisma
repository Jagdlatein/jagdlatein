model QuizQuestion {
  id        String   @id @default(uuid())
  country   String?
  category  String?
  topic     String?
  question  String
  optionA   String
  optionB   String
  optionC   String
  optionD   String
  correct   String   @db.VarChar(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
